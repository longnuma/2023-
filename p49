#include <stdio.h>

int main(void)
{
	int a, b, surplus, result;
	
	//２つの整数を入力
	printf("Please Enter Number:");
	scanf_s("%d", &a);
	printf("`Please Enter Other Number:");
	scanf_s("%d", &b);

	//余りを計算
	surplus = a % b;

	//条件分岐
	if (surplus == 0)
	{
		result = a / b;
		printf("%d / %d = %d\n", a, b, result);
	}

 	return 0;
}
